@prefix map: <file:/C:/Users/Luis/Documents/Mestrado/d2r-server-0.7/mapping-local.n3#> .
@prefix db: <> .
@prefix vocab: <http://localhost:2020/vocab/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .

map:database a d2rq:Database;
	d2rq:jdbcDriver "com.mysql.jdbc.Driver";
	d2rq:jdbcDSN "jdbc:mysql://127.0.0.1/ebay";
	d2rq:username "root";
	d2rq:password "admin";
	jdbc:autoReconnect "true";
	jdbc:zeroDateTimeBehavior "convertToNull";
	.

# Table offer
map:offer a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "offer/@@offer.id@@";
	d2rq:class vocab:offer;
	d2rq:classDefinitionLabel "offer";
	.
map:offer__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property rdfs:label;
	d2rq:pattern "offer #@@offer.id@@";
	.
map:offer_id a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_id;
	d2rq:propertyDefinitionLabel "offer id";
	d2rq:column "offer.id";
	d2rq:datatype xsd:int;
	.
map:offer_qty a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_qty;
	d2rq:propertyDefinitionLabel "offer qty";
	d2rq:column "offer.qty";
	d2rq:datatype xsd:int;
	.
map:offer_price a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_price;
	d2rq:propertyDefinitionLabel "offer price";
	d2rq:column "offer.price";
	d2rq:datatype xsd:decimal;
	.
map:offer_currency a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_currency;
	d2rq:propertyDefinitionLabel "offer currency";
	d2rq:column "offer.currency";
	.
map:offer_product a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_product;
	d2rq:refersToClassMap map:product;
	d2rq:join "offer.product => product.title";
	.
map:offer_seller a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:offer;
	d2rq:property vocab:offer_seller;
	d2rq:refersToClassMap map:seller;
	d2rq:join "offer.seller => seller.name";
	.


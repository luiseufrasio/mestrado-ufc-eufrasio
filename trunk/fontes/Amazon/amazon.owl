<?xml version='1.0'?>
<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns="http://sites.google.com/site/luiseufrasio/amazon.owl"
>
    
    <owl:Ontology rdf:about="">
        <owl:versionInfo>$Id: amazon.owl,v 1.0 2011/04/25 21:12:40 meh Exp $</owl:versionInfo>        <rdfs:comment>Amazon Ontology</rdfs:comment>
    </owl:Ontology>
    
    <owl:Class rdf:ID="Product">
        <rdfs:subClassOf>
            <owl:Restriction>                <owl:onProperty rdf:resource="#title" />            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:Class rdf:ID="Publ">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#name" />
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#address" />
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:Class rdf:ID="Music">
        <rdfs:subClassOf rdf:resource="#Product" />
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#recorder" />
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:Class rdf:ID="Book">
        <rdfs:subClassOf rdf:resource="#Product" />
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="pub" rdf:datatype="#Publ"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:ObjectProperty rdf:ID="title" rdf:datatype="xsd:string" />
    
    <owl:ObjectProperty rdf:ID="name" rdf:datatype="xsd:string" />
    
    <owl:ObjectProperty rdf:ID="address" rdf:datatype="xsd:string" />
    
    <owl:ObjectProperty rdf:ID="recorder" rdf:datatype="xsd:string" />
    
</rdf:RDF>